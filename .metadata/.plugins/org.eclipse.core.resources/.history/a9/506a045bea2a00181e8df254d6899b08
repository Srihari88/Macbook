	String ename;

	public Employee(int eid, String ename) {
		// TODO Auto-generated constructor stub

		this.eid = eid;
		this.ename = ename;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Employee e1 = new Employee(111, "Pudu");

	}

}
