package Arrays;

import java.util.ArrayList;

public class SriHari {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ArrayList al = new ArrayList();

		al.add("SriHari");
		al.add("Naidu");

		al.add("Pudu");
		al.add(10.5);
		al.add(900);

		System.out.println("The object is arrayList" + al);
		
		al.add(2,100);

		System.out.println("The object is arrayList" + al);
	}

}
