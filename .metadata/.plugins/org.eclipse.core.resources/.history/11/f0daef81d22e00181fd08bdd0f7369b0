package ArrayProgramsPractice;

public class SumOfTwoArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[][] a = { { 5, 6, 8 } };
		int[][] b = { { 8, 6, 8 } };

		int[][] c = new int[2][3];

		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < 3; j++) {
				c[i][j] = a[i][j] + b[i][j];
				System.out.print(c[i][j] + " ");
			}
			System.out.println();// new line
		}

	}

}
