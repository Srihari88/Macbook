
// Write a program to print 1 to 100 numbers in java
public class Tough {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 5; i > 0; i--) {

			for (int j = 0; j < i; j++) {
				System.out.print("*");
			}
			System.out.println("");
		}

		//

		for (int i = 0; i < 5; i++) {
			for (int j = 0; i < j; j++) {
				System.out.print("*");
			}

		}
		System.out.println("");

	}
}
