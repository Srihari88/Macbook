package ArrayProgramsPractice;

public class SumOfTwoArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[][] a = { { 5, 6, 8 }, { 5, 7, 9 } };
		int[][] b = { { 8, 6, 8 }, { 1, 2, 93 } };

		int[][] c = new int[3][2];

		for (int i = 0; i < 2; i++) {
			for (j = 0; i < 3; j++) {
				c[i][j] = a[i][j] + b[i][j];
			}
		}

	}

}
