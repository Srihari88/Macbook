package HashMap;

import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Set;
import java.util.TreeMap;

public class Loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		HashMap<Integer, String> hashmap = new HashMap<Integer, String>();

		hashmap.put(111, "CHIRU");
		hashmap.put(222, "RAM CHARAN");
		hashmap.put(333, "PAWAN KALYAN");
		hashmap.put(444, " ALLU ARJUN");

		Set total = hashmap.entrySet();

		Iterator itr = total.iterator();
		while (itr.hasNext()) {
			Map.Entry sri = (Map.Entry) itr.next();
			System.out.println(sri);

		}

		for (Map.Entry ss : hashmap.entrySet()) {
			System.out.println("The Value is" + ss.getKey() + "THe Value is" + ss.getValue());
		}

		Map t = new TreeMap(hashmap);

		System.out.println(hashmap);

		// HashMap<Integer, String> hmap = new HashMap<Integer, String>();
