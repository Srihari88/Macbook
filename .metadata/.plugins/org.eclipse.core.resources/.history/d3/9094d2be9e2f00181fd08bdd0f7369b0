package HashMap;

import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Set;

public class Loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * HashMap<Integer, String> hashmap = new HashMap<Integer, String>();
		 * 
		 * hashmap.put(111, "CHIRU"); hashmap.put(222, "RAM CHARAN");
		 * hashmap.put(333, "PAWAN KALYAN"); hashmap.put(444, " ALLU ARJUN");
		 * 
		 * Set total = hashmap.entrySet();
		 * 
		 * Iterator itr = total.iterator(); while (itr.hasNext()) { Map.Entry
		 * sri = (Map.Entry) itr.next(); System.out.println(sri);
		 * 
		 * }
		 */

		HashMap<Integer, String> hmap = new HashMap<Integer, String>();
		// Adding elements to HashMap
		hmap.put(11, "AB");
		hmap.put(2, "CD");
		hmap.put(33, "EF");
		hmap.put(9, "GH");
		hmap.put(3, "IJ");

		// FOR LOOP
		System.out.println("For Loop:");
		for (Map.Entry me : hmap.entrySet()) {
			System.out.println("Key: " + me.getKey() + " & Value: " + me.getValue());
		}
	}

}
