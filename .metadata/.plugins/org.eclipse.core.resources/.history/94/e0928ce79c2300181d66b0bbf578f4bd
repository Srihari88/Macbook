
// Write a program to print 1 to 100 numbers in java
public class Tough {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int limit = 100;

		for (int i = 1; i <= 100; i++) {

			boolean isEven = true;

			for (int j = 2; j < i; j++) {

				if (i % j == 0) {
					isEven = false;
					break;
				}

				if (isEven) {
					System.out.println(i);
				}
			}
		}

	}

}
