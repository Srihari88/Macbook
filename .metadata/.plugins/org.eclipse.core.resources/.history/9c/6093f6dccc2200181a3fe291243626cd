import java.util.Scanner;

public class Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a = 20;

		switch (a) {

		case 20:
			System.out.println(" Age 18");
			break;

		case 30:

			System.out.println(" Age 19");
			break;
		default:

			System.out.println(" Not matched");
			break;
		}
	}

}
