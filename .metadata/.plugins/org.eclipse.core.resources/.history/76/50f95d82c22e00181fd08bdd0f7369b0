package ArrayProgramsPractice;

public class Employee {

	int eid;
	String ename;

	Employee(int eid, String ename) {
		this.eid = eid;
		this.ename = ename;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Employee e1 = new Employee(111, "Srihari");
		Employee e2 = new Employee(222, "Naidu");
		Employee e3 = new Employee(333, "Pudu");

		Employee[] e = new Employee[5];

		e[0] = e1;
		e[1] = e2;
		e[2] = e3;

		for (Object ee : e) {
			if (ee instanceof Employee) {

				Employee s = (Employee) ee;
				System.out.println();
			}
		}
	}
}
